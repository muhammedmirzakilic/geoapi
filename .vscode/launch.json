    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "node",
            "request": "launch",
            "args": ["${workspaceFolder}/src/main.ts"],
            "runtimeArgs": ["-r", "ts-node/register"],
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart",
            "outputCapture": "std",
            "env": {
              "TS_NODE_IGNORE": "false"
            }
          }
    ]
}